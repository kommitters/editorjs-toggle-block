{
  "name": "editorjs-toggle-block",
  "version": "0.3.14",
  "description": "Toggle block tool for Editor.js",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kommitters/editorjs-toggle-block.git"
  },
  "keywords": [
    "toggle",
    "block",
    "tool",
    "editor.js",
    "editorjs"
  ],
  "author": {
    "name": "kommitters Open Source",
    "email": "oss@kommit.co"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kommitters/editorjs-toggle-block/issues"
  },
  "homepage": "https://github.com/kommitters/editorjs-toggle-block#readme",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@testing-library/jest-dom": "^6.0.0",
    "babel-jest": "^29.0.2",
    "babel-loader": "^9.0.0",
    "css-loader": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.8.2",
    "typescript": "^5.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(css|svg)$": "<rootDir>/test/config/assetsTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!uuid)"
    ]
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
